{
  "private": true,
  "name": "scribubble",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "preact build",
    "serve": "sirv build --port 4000 --cors --single",
    "dev": "preact watch --port 8888",
    "dev-safe": "preact  watch --port 8888 --https --key localhost-key.pem --cert localhost.pem",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^8.7.0",
    "eslint-config-preact": "^1.1.0",
    "jest": "^27.4.7",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.3.3",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "@seregpie/three.text-sprite": "^3.2.0",
    "aframe": "^1.2.0",
    "preact": "^10.3.2",
    "preact-async-route": "^2.2.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "socket.io-client": "^4.4.0",
    "styled-components": "^5.3.3"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
